{
  "name": "dekodeinteraktiv/savage-cards",
  "description": "Card setup for Dekode design framework",
  "homepage": "https://github.com/DekodeInteraktiv/savage-cards",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.2"
  },
  "archive": {
    "exclude": [
      ".editorconfig",
      ".gitignore",
      "composer.json",
      "composer.lock",
      "LICENSE",
      "phpcs.xml"
    ]
},
  "require-dev": {
    "dekodeinteraktiv/coding-standards": "^0.3.1"
  },
  "scripts" : {
